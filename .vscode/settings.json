{
  "files.associations": {
    "*.pro.md": "markdown"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[*.pro.md]": {
    "editor.formatOnSave": false
  },
  "prettier.documentSelectors": ["**/*.pro.md"],
  "prettier.ignorePath": ".prettierignore"
}
